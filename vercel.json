{
  "version": 2,
  "public": true,
  "rewrites": [
    {
      "source": "/css/(.*)",
      "destination": "/web/css/$1"
    },
    {
      "source": "/js/(.*)",
      "destination": "/web/js/$1"
    },
    {
      "source": "/public/(.*)",
      "destination": "/web/public/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/web/index.html"
    }
  ],
  "headers": [
    {
      "source": "/public/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=300, s-maxage=300"
        },
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    },
    {
      "source": "/css/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, s-maxage=3600"
        },
        {
          "key": "Content-Type",
          "value": "text/css"
        }
      ]
    },
    {
      "source": "/js/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, s-maxage=3600"
        },
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ]
}